{
    "name": "uit.web.store",
    "version": "0.0.1",
    "description": "Edit Readme File",
    "main": "dist/server/app.js",
    "author": "Phạm Hồng Phúc <phamhongphuc.info@gmail.com>",
    "license": "MIT",
    "scripts": {
        "heroku-postbuild": "yarn run build",
        "lint": "eslint src test",
        "lint:skip": "eslint --rule \"no-unused-vars: 0\" src test",
        "lint:fix": "eslint src test --fix",
        "style": "scss --watch style/index.scss:dist/render.css",
        "style-render": "scss --style compressed style/index.scss:dist/render.css",
        "clean": "rimraf database && mkdir database",
        "build:server": "babel src/server -d dist/server",
        "build:client": "babel src/webpack -d dist/webpack && webpack --config dist/webpack/webpack.prod.js",
        "build": "cross-env NODE_ENV=production npm-run-all build:server build:client",
        "start": "node dist/server/app",
        "run": "cross-env NODE_ENV=production node dist/server/app",
        "dev": "cross-env NODE_ENV=development nodemon src/server/app.js --exec babel-node",
        "one": "cross-env NODE_ENV=development babel-node src/server/app.js",
        "test": "mocha test --recursive --exit --require mocha-babel",
        "test:one": "mocha test/run --recursive --exit --require mocha-babel",
        "test:now": "mocha test/api/importCoupon.test.js --exit --require mocha-babel",
        "chrome": "Start-Process \"chrome.exe\" \"ai.ai\" --unsafely-treat-insecure-origin-as-secure=http://ai.ai/"
    },
    "nodemonConfig": {
        "watch": [
            "src/server",
            "src/webpack",
            "dist"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/phamhongphuc/uit.web.store.git"
    },
    "bugs": {
        "url": "https://github.com/phamhongphuc/uit.web.store/issues"
    },
    "homepage": "https://github.com/phamhongphuc/uit.web.store#readme",
    "engines": {
        "node": "8.11.2"
    },
